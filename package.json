{
  "name": "mzawadi-bots",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "clear-pub": "rm -rfv public/assets/*.js public/assets/*.css",
    "sync-pub": "rsync -av --progress dist/ public/",
    "zip-pub": "rm -rfv public.zip && zip -rv public.zip public",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@types/animejs": "^3.1.12",
    "@types/marked": "^5.0.1",
    "@vue-hero-icons/outline": "^1.7.2",
    "@vue-hero-icons/solid": "^1.7.2",
    "@vueuse/core": "^10.4.1",
    "animejs": "^3.2.1",
    "dompurify": "^3.1.0",
    "gsap": "^3.12.3",
    "highlight.js": "^11.8.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "marked": "^8.0.1",
    "moment": "^2.30.1",
    "nodemon": "^3.1.0",
    "pinia": "^2.1.6",
    "recordrtc": "^5.6.2",
    "socket-io": "^1.0.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "uuid": "^9.0.1",
    "uuidv4": "^6.2.13",
    "vee-validate": "^4.11.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-google-login": "^2.0.25"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/dompurify": "^3.0.5",
    "@types/jquery": "^3.5.29",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.28",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "vite": "^5.1.6",
    "vite-plugin-vue-devtools": "^7.0.18",
    "vue-tsc": "^2.0.6"
  }
}
